{
	"name": "@wymjs/vite-mock-api",
	"version": "1.0.0",
	"type": "module",
	"description": "簡單的 mock api 功能",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"readme.md"
	],
	"scripts": {
		"build": "tsup-node index.ts --platform node --format esm --dts --clean",
		"publish-package": "pnpm run build && npm publish --access public",
		"lint": "eslint --fix --no-ignore --max-warnings=0",
		"format": "prettier --cache --write",
		"prepare": "husky install"
	},
	"homepage": "https://github.com/wymjs/vite-mock-api",
	"keywords": [
		"wymjs",
		"typescript",
		"vite",
		"mock",
		"api"
	],
	"license": "ISC",
	"lint-staged": {
		"*.{js,jsx,ts,tsx,vue}": "pnpm run lint",
		"*.{js,jsx,ts,tsx,vue,css,sass,scss,less}": "pnpm run format"
	},
	"dependencies": {
		"vite": "^5.0.10",
		"query-string": "^8.1.0",
		"body": "^5.1.0"
	},
	"devDependencies": {
		"@types/body": "^5.1.4",
		"@types/node": "^20.9.0",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"eslint": "^8.57.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.1.0",
		"prettier": "^3.1.0",
		"typescript": "^5.2.2",
		"tsup": "^8.0.1"
	},
	"peerDependencies": {
		"vite": ">=5",
		"query-string": ">=8",
		"body": ">=5"
	}
}
